{
    "manifest_version": 3,
    "type": "extension",
    "name": "消息靠左连续展示",
    "slug": "message_show",
    "description": "类似QQ频道内的消息展示结构, 所有消息在左侧连续展示",
    "version": "1.2",
    "thumbnail": "./res/icon.jpg",
    "author": {
        "name": "恨别鸟惊心_",
        "link": "https://github.com/ImZjm"
    },
    "repository": {
        "repo": "ImZjm/LiteLoaderQQNT-Message-Show",
        "branch": "main"
    },
    "platform": [
        "win32",
        "linux",
        "darwin"
    ],
    "injects": {
        "renderer": "./src/renderer.js",
        "main": "./src/main.js",
        "preload": "./src/preload.js"
    }
}